extends layout

block content
  block content
  h1= title
  - var image_names = ["sail_powered_boats.jpg","powered_boats.jpg","motor_boats.jpg","speed_boats.jpg", "pedal.jpg"];
  - for(i=0;i<3;i++) {
     - var randNum = Math.floor(Math.random()*5) ;
     img(src=`./images/${image_names[randNum]}` alt=`img${image_names[randNum]}` style='width:300px; height:300px;')

  -}
